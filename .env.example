# Display name in notifications
CLAW_NAME=OpenClaw

# Notify interval in ms (0 = disabled, default: 3600000 = 1 hour)
NOTIFY_INTERVAL_MS=3600000

# Quiet hours: skip healthy notifications during these hours (24h format, default 23-8)
QUIET_HOURS_START=23
QUIET_HOURS_END=10

# Auto-written by init; absolute project path
PROJECT_DIR=

# Optional: leave empty to auto-detect (~/.openclaw/openclaw.json)
OPENCLAW_CONFIG_PATH=
HEALTH_URL=http://localhost:18789/health
CHECK_INTERVAL_MS=300000
FAIL_THRESHOLD=2
ROLLBACK_THRESHOLD=2

AUTO_RESTART=true
RESTART_COMMAND=openclaw gateway restart

AUTO_ROLLBACK=true
# Use `internal` for built-in cross-platform rollback (recommended)
ROLLBACK_COMMAND=internal

NOTIFIER=telegram

TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

DISCORD_WEBHOOK_URL=

WHATSAPP_WEBHOOK_URL=
WHATSAPP_WEBHOOK_TOKEN=
